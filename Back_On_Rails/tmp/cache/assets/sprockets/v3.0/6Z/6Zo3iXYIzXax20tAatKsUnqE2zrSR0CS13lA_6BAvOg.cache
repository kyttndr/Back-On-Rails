I"Í(function() {
  var ready_post;

  ready_post = function() {
    var readURL;
    $('.photo_upload').on('change', function(e) {
      return readURL(this);
    });
    return readURL = function(input) {
      var reader;
      if (input.file && input.files[0]) {
        reader = new FileReader();
      }
      reader.onload = function(e) {
        var $swap;
        $('.photo_upload').attr('src', e.target.result).removeClass('hidden');
        $swap = $('.swap');
        if ($swap) {
          return $swap.removeClass('hidden');
        }
      };
      return reader.readAsDataURL(input.files[0]);
    };
  };

  $(document).ready(ready_post);

  $(document).on('turbolinks:load', ready_post);

}).call(this);
