<% if @users %>
  <div id="friend-lookup-results" class="well results-block col-md-10">
    <table class="search-results-table col-md-12">
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><strong>Name:</strong> <%= user.username %></td>
            <td><strong>Email:</strong> <%= user.email %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
<% end %>